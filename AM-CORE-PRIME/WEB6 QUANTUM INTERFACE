    <script>
        // Neuro Cursor System
        const cursor = document.getElementById('neuroCursor');
        let trails = [];

        document.addEventListener('mousemove', (e) => {
            // Main cursor
            cursor.style.left = e.clientX + 'px';
            cursor.style.top = e.clientY + 'px';
            
            // Cursor trail
            const trail = document.createElement('div');
            trail.className = 'cursor-trail';
            trail.style.left = e.clientX + 'px';
            trail.style.top = e.clientY + 'px';
            document.body.appendChild(trail);
            
            trails.push(trail);
            
            // Remove old trails
            if (trails.length > 10) {
                const oldTrail = trails.shift();
                if (oldTrail && oldTrail.parentNode) {
                    oldTrail.parentNode.removeChild(oldTrail);
                }
            }
            
            // Particle effect
            createParticles(e.clientX, e.clientY);
        });

        function createParticles(x, y) {
            for (let i = 0; i < 3; i++) {
                const particle = document.createElement('div');
                particle.className = 'particle';
                particle.style.left = x + 'px';
                particle.style.top = y + 'px';
                particle.style.setProperty('--tx', (Math.random() - 0.5) * 100 + 'px');
                particle.style.setProperty('--ty', (Math.random() - 0.5) * 100 + 'px');
                particle.style.width = Math.random() * 4 + 2 + 'px';
                particle.style.height = particle.style.width;
                
                document.body.appendChild(particle);
                
                setTimeout(() => {
                    if (particle.parentNode) {
                        particle.parentNode.removeChild(particle);
                    }
                }, 2000);
            }
        }

        // Matrix Background
        function createMatrixBackground() {
            const matrixBg = document.getElementById('matrixBg');
            const columns = Math.floor(window.innerWidth / 20);
            
            for (let i = 0; i < columns; i++) {
                const column = document.createElement('div');
                column.className = 'matrix-column';
                column.style.left = (i * 20) + 'px';
                column.style.animationDelay = (Math.random() * 3) + 's';
                column.style.opacity = Math.random() * 0.3 + 0.1;
                matrixBg.appendChild(column);
            }
        }

        // User Counter Simulation
        function updateUserCounter() {
            const counter = document.getElementById('userCount');
            let count = 1000 + Math.floor(Math.random() * 9000);
            counter.textContent = count.toString().padStart(4, '0');
        }

        // Neuro Sync Activation
        function activateNeuroSync() {
            const button = event.target;
            button.textContent = '–ù–ï–ô–†–û-–°–ò–ù–•–†–û–ù–ò–ó–ê–¶–ò–Ø –ê–ö–¢–ò–í–ò–†–û–í–ê–ù–ê';
            button.style.background = 'linear-gradient(135deg, #00ff88 0%, #008844 100%)';
            
            // Simulate neuro connection
            document.body.style.animation = 'pulse 0.147s infinite';
            
            setTimeout(() => {
                document.body.style.animation = '';
            }, 2000);
        }

        // Initialize
        document.addEventListener('DOMContentLoaded', () => {
            createMatrixBackground();
            updateUserCounter();
            setInterval(updateUserCounter, 5000);
        });
        
        // ---- WebAudio: binaural beats to produce 6.8 Hz beat (220Hz left / 226.8Hz right) ----
          let audioCtx, leftOsc, rightOsc, leftGain, rightGain, masterGain;
        function startBinaural(){
          if(audioCtx) return;
          audioCtx = new (window.AudioContext || window.webkitAudioContext)();
          masterGain = audioCtx.createGain(); masterGain.gain.value = 0.02; // very subtle
          leftGain = audioCtx.createGain(); rightGain = audioCtx.createGain();
          leftGain.gain.value = 1; rightGain.gain.value = 1;

        // create stereo panner nodes to route left/right
          const pannerL = audioCtx.createStereoPanner(); pannerL.pan.value = -1;
          const pannerR = audioCtx.createStereoPanner(); pannerR.pan.value = 1;
          leftOsc = audioCtx.createOscillator(); leftOsc.type = 'sine'; leftOsc.frequency.value = 220; // base
          rightOsc = audioCtx.createOscillator(); rightOsc.type = 'sine'; rightOsc.frequency.value = 226.8; // base + 6.8 Hz -> binaural beat
          leftOsc.connect(leftGain); leftGain.connect(pannerL); pannerL.connect(masterGain);
          rightOsc.connect(rightGain); rightGain.connect(pannerR); pannerR.connect(masterGain);
          masterGain.connect(audioCtx.destination);
          leftOsc.start(); rightOsc.start();
       }
    
// Start audio on first user gesture (browser autoplay policies)
    window.addEventListener('click', startBinaural, { once:true });
        // Smooth scroll for navigation
        document.querySelectorAll('.nav-node').forEach(anchor => {
            anchor.addEventListener('click', function(e) {
                e.preventDefault();
                const targetId = this.getAttribute('href');
                const targetElement = document.querySelector(targetId);
                
                if (targetElement) {
                    targetElement.scrollIntoView({
                        behavior: 'smooth',
                        block: 'start'
                    });
                }
            });
        });
    </script>
